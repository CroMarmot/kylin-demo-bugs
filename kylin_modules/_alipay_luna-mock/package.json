        "eslint-plugin-standard": "^2.0.1",
        "istanbul": "~0.4.5",
        "jsdom": "~9.5.0",
        "mocha": "~3.1.0",
        "rollup": "^0.38.0",
        "rollup-plugin-buble": "^0.15.0",
