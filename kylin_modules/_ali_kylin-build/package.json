{
    "name": "@ali/kylin-build",
    "version": "0.1.49-open01",
    "description": "kylin build",
    "author": "",
    "bin": {
        "kylin-build": "./bin/kylin-build"
    },
    "publishConfig": {
        "registry": "http://registry.npm.alibaba-inc.com"
    },
    "files": [
        "bin",
        "lib"
    ],
    "scripts": {
        "build": "rm -rf lib && cross-env BABEL_ENV=KYLIN_BUILD babel src --out-dir lib --copy-files",
        "dev": "rm -rf lib && cross-env BABEL_ENV=KYLIN_BUILD babel src --out-dir lib --watch -s --copy-files",
        "test": "cross-env NODE_ENV=test BABEL_ENV=KYLIN_TEST node_modules/.bin/mocha test/*.test.js",
        "test:cov": "rm -rf ./coverage && cross-env NODE_ENV=test BABEL_ENV=KYLIN_TEST istanbul cover _mocha test/*.test.js",
        "debug": "$(npm bin)/mocha --require babel-core/register --no-timeouts",
        "beta": "tnpm publish --tag beta",
        "open": "tnpm publish --tag open",
        "lint": "eslint ./src/ --ext js",
        "lint:fix": "eslint --fix ./src/ --ext js",
        "ci": "tnpm run lint && tnpm run test:cov && alicov"
    },
